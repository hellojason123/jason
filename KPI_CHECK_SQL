insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('CAS_CHK_019', '1', 'CAS_Check', '', 'Y', 'SELECT /*+ PARALLEL(8)*/COUNT(1) FROM( 
 SELECT 1 FROM SRC_MED.PRE_CAS_USING_CUST_ID B
  WHERE NOT EXISTS(SELECT 1  FROM SRC_MED.PRE_CAS_ALL_INFO T 
  JOIN SRC_MED.PRE_CAS_SUBS_ID A
  ON T.SUBS_ID = A.S_SUBS_ID
  WHERE T.ACTUAL_USER_FLAG = ''Y'' AND B.S_SUBS_ID = T.SUBS_ID)
UNION ALL
  SELECT 1 FROM SRC_MED.PRE_CAS_SME B
  WHERE NOT EXISTS(SELECT 1  FROM SRC_MED.PRE_CAS_ALL_INFO_SME T 
  JOIN SRC_MED.PRE_CAS_SME A
  ON T.SUBS_ID = A.S_SUBS_ID
  AND A.S_SUBS_ID IS NOT NULL
  WHERE T.ACTUAL_USER_FLAG = ''Y'' AND B.S_SUBS_ID = T.SUBS_ID)
  AND B.S_USING_CUST_ID IS NOT NULL)');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('CAS_CHK_018_1', '1', 'CAS_Check', '', 'Y', 'Select /*+ parallel(4)*/Count(1)
  From s3_data.mid_subs a
  join s3_data.mid_actual_user d
    on a.cust_id = d.subs_id
  join s3_data.mid_cust e
    on e.ident_type_code = d.ident_type_code
   and e.ident_no = d.ident_no
   and e.cust_role = ''31416''');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('CAS_CHK_018_2', '1', 'CAS_Check', '', 'Y', 'SELECT COUNT(1) FROM(  
SELECT 1  FROM SRC_MED.PRE_CAS_ALL_INFO T 
  JOIN SRC_MED.PRE_CAS_SUBS_ID A
  ON T.SUBS_ID = A.S_SUBS_ID
  WHERE T.ACTUAL_USER_FLAG = ''Y''
UNION ALL
SELECT 1  FROM SRC_MED.PRE_CAS_ALL_INFO_SME T 
  JOIN SRC_MED.PRE_CAS_SME A
  ON T.SUBS_ID = A.S_SUBS_ID
  AND A.S_SUBS_ID IS NOT NULL
  WHERE T.ACTUAL_USER_FLAG = ''Y'')');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('CAS_CHK_022', '1', 'CAS_Check', '', 'Y', 'SELECT /*+ PARALLEL(4)*/COUNT(1) FROM SRC_MED.DM_ADDR_REF T WHERE T.ADDR_REF_PURPOSE_ID = ''6010506003'' AND T.ADDR_REF_ENTITY_ID LIKE ''32%''');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('DL_CHK_003_1', '1', 'CAS_Check', '', 'Y', 'SELECT COUNT(1) FROM S3_DATA.MID_ACCT_ADDR_REL');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('DL_CHK_003_2', '1', 'CAS_Check', '', 'Y', 'SELECT COUNT(1) FROM TEST_TLF_FIXED_DATA.MID_ACCT_ADDR_REL A
 WHERE EXISTS(SELECT * FROM SRC_MED.PRE_CAS_SME B WHERE A.ACCT_ID = B.S_ACCT_ID)');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('DL_CHK_027', '1', 'CAS_Check', '', 'Y', 'SELECT COUNT(1) FROM CUST_USER.INF_CUST_SRV_CHANNEL_CZ');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('DL_CHK_026', '1', 'CAS_Check', '', 'Y', 'SELECT COUNT(1) FROM CUST_USER.INF_CUST_CONTRACT_CZ');

insert into exe_fixed.kpi_check_sql (KPI_ID, GROUP_ID, KPI_TYPE, KPI_DESC, CHK_FLAG, KPI_SQL)
values ('DL_CHK_025', '1', 'CAS_Check', '', 'Y', 'SELECT /*+ USE_HASH(A,B)*/COUNT(1)
  FROM TEST_TLF_FIXED_DATA.MID_ORG_CONTACT A
  JOIN TEST_TLF_FIXED_DATA.MID_CUST B
    ON A.IDENT_TYPE = B.IDENT_TYPE_NAME
   AND A.IDENT_NO = B.IDENT_NO
  JOIN (SELECT DISTINCT S_CUST_ID, T_CUST_ID, FLAG_POST_ACCT
          FROM SRC_MED.PRE_CAS_SME) C
    ON A.CUST_ID = C.S_CUST_ID');

