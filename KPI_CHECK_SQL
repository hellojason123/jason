create or replace package body PKG_GRETA21_KPI is

  procedure P_KPI_TOOL is
    v_sql   varchar2(2048);
    v_cnt   number(20);
  begin
    v_sql := 'truncate table exe_fixed.kpi_check_result';
    execute immediate v_sql;
    
    for kpi_check in (select * from exe_fixed.kpi_check_sql t where t.chk_flag = 'Y') loop
      v_sql := kpi_check.kpi_sql;
      begin
      execute immediate v_sql into v_cnt;
      exception
        when others then
          NULL;
      end;
      
     
      

      insert into exe_fixed.kpi_check_result values
      (kpi_check.kpi_id,
       kpi_check.group_id,
       kpi_check.kpi_type,
       kpi_check.kpi_desc,
       v_cnt,
       sysdate);
       commit;
     

    end loop;
  end P_KPI_TOOL;

end PKG_GRETA21_KPI;


create table KPI_CHECK_SQL
(
  kpi_id   VARCHAR2(32) not null,
  group_id NUMBER(1),
  kpi_type VARCHAR2(32),
  kpi_desc VARCHAR2(64),
  chk_flag VARCHAR2(1) not null,
  kpi_sql  VARCHAR2(2048) not null
)
tablespace EXE_FIXED_DAT
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
  
create table KPI_CHECK_RESULT
(
  kpi_id    VARCHAR2(32),
  group_id  NUMBER(1),
  kpi_type  VARCHAR2(32),
  kpi_desc  VARCHAR2(64),
  amt_count NUMBER(20),
  chk_date  DATE
)
tablespace EXE_FIXED_DAT
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
  
